{
	"root": true,
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"plugins": [
		"react"
	],
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"rules": {
		"no-console": "off",
		"semi": [
			"error",
			"always"
		],
		"quotes": [
			"error",
			"single"
		]
	}
}
